<Window x:Class="_23._01.Window_Kolor"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:_23._01"
        mc:Ignorable="d"
        Title="Window_Kolor" Height="450" Width="800">
    <StackPanel>
        <Label>R:</Label>
        <TextBox x:Name="kolor_r"/>
        <Label>G:</Label>
        <TextBox x:Name="kolor_g"/>
        <Label>B:</Label>
        <TextBox x:Name="kolor_b"/>
        <Button Click="Button_Click_Zapisz">Zapisz</Button>
    </StackPanel>
</Window>
